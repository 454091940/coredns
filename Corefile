. {
    root /etc/coredns
    git github.com/454091940/coredns {
        branch      main
        interval    60
    }
}

deepmirror.com {
    file /etc/coredns/deepmirror.com/file
    errors
    log
}

example.org:53 {
    file /etc/coredns/db.example.org
    errors
    log
}

.:53 {
    hosts /etc/coredns/hostsfile {
        fallthrough
    }
    forward . 8.8.8.8:53
    log
}
